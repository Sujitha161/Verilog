#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x561c0fec4510 .scope module, "tbhalf" "tbhalf" 2 1;
 .timescale 0 0;
v0x561c0fed5f90_0 .var "a", 0 0;
v0x561c0fed6050_0 .var "b", 0 0;
v0x561c0fed6120_0 .net "carry", 0 0, L_0x561c0fed6450;  1 drivers
v0x561c0fed6220_0 .net "sum", 0 0, L_0x561c0fed62f0;  1 drivers
S_0x561c0fec46a0 .scope module, "uut" "halfadder" 2 4, 3 1 0, S_0x561c0fec4510;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "a";
    .port_info 1 /INPUT 1 "b";
    .port_info 2 /OUTPUT 1 "sum";
    .port_info 3 /OUTPUT 1 "carry";
L_0x561c0fed62f0 .functor XOR 1, v0x561c0fed5f90_0, v0x561c0fed6050_0, C4<0>, C4<0>;
L_0x561c0fed6450 .functor AND 1, v0x561c0fed5f90_0, v0x561c0fed6050_0, C4<1>, C4<1>;
v0x561c0fec48f0_0 .net "a", 0 0, v0x561c0fed5f90_0;  1 drivers
v0x561c0fed5cf0_0 .net "b", 0 0, v0x561c0fed6050_0;  1 drivers
v0x561c0fed5db0_0 .net "carry", 0 0, L_0x561c0fed6450;  alias, 1 drivers
v0x561c0fed5e50_0 .net "sum", 0 0, L_0x561c0fed62f0;  alias, 1 drivers
    .scope S_0x561c0fec4510;
T_0 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x561c0fed5f90_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x561c0fed6050_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x561c0fed5f90_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x561c0fed6050_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x561c0fed5f90_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x561c0fed6050_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x561c0fed5f90_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x561c0fed6050_0, 0, 1;
    %delay 10, 0;
    %vpi_call 2 12 "$finish" {0 0 0};
    %end;
    .thread T_0;
    .scope S_0x561c0fec4510;
T_1 ;
    %vpi_call 2 15 "$dumpfile", "out_halfadder.vcd" {0 0 0};
    %vpi_call 2 16 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x561c0fec4510 {0 0 0};
    %vpi_call 2 17 "$monitor", "time=%0t,a=%b,b=%b,sum=%b,carry=%b", $time, v0x561c0fed5f90_0, v0x561c0fed6050_0, v0x561c0fed6220_0, v0x561c0fed6120_0 {0 0 0};
    %end;
    .thread T_1;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "tb_half.v";
    "halfadder.v";
