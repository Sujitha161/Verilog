#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x63469d85da10 .scope module, "tb_count" "tb_count" 2 1;
 .timescale 0 0;
v0x63469d883b20_0 .var "clk", 0 0;
v0x63469d883bf0_0 .net "count", 3 0, v0x63469d883920_0;  1 drivers
v0x63469d883cc0_0 .var "rst", 0 0;
S_0x63469d8721a0 .scope module, "uut" "syncounter" 2 4, 3 1 0, S_0x63469d85da10;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /INPUT 1 "rst";
    .port_info 2 /OUTPUT 4 "count";
v0x63469d8723f0_0 .net "clk", 0 0, v0x63469d883b20_0;  1 drivers
v0x63469d883920_0 .var "count", 3 0;
v0x63469d883a00_0 .net "rst", 0 0, v0x63469d883cc0_0;  1 drivers
E_0x63469d872820 .event posedge, v0x63469d8723f0_0;
    .scope S_0x63469d8721a0;
T_0 ;
    %wait E_0x63469d872820;
    %load/vec4 v0x63469d883a00_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 0, 0, 4;
    %assign/vec4 v0x63469d883920_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x63469d883920_0;
    %addi 1, 0, 4;
    %assign/vec4 v0x63469d883920_0, 0;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x63469d85da10;
T_1 ;
    %delay 5, 0;
    %load/vec4 v0x63469d883b20_0;
    %inv;
    %store/vec4 v0x63469d883b20_0, 0, 1;
    %jmp T_1;
    .thread T_1;
    .scope S_0x63469d85da10;
T_2 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x63469d883b20_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x63469d883cc0_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x63469d883cc0_0, 0, 1;
    %delay 50, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x63469d883cc0_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x63469d883cc0_0, 0, 1;
    %delay 100, 0;
    %vpi_call 2 12 "$finish" {0 0 0};
    %end;
    .thread T_2;
    .scope S_0x63469d85da10;
T_3 ;
    %vpi_call 2 15 "$dumpfile", "out_syncount" {0 0 0};
    %vpi_call 2 16 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x63469d85da10 {0 0 0};
    %vpi_call 2 17 "$monitor", "time=%0t,clk=%b,rst=%b,count=%b", $time, v0x63469d883b20_0, v0x63469d883cc0_0, v0x63469d883bf0_0 {0 0 0};
    %end;
    .thread T_3;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "tb_sync_counter.v";
    "sync_counter.v";
