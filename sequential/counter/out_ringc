#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x6457ccbab740 .scope module, "tb_ringc" "tb_ringc" 2 1;
 .timescale 0 0;
v0x6457ccbbe010_0 .var "clk", 0 0;
v0x6457ccbbe0d0_0 .net "count", 3 0, v0x6457ccbbdcf0_0;  1 drivers
v0x6457ccbbe1a0_0 .var "ori", 0 0;
v0x6457ccbbe2a0_0 .var "rst", 0 0;
S_0x6457ccbab8d0 .scope module, "uut" "ringcount" 2 5, 3 1 0, S_0x6457ccbab740;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /INPUT 1 "rst";
    .port_info 2 /INPUT 1 "ori";
    .port_info 3 /OUTPUT 4 "count";
v0x6457ccb70490_0 .net "clk", 0 0, v0x6457ccbbe010_0;  1 drivers
v0x6457ccbbdcf0_0 .var "count", 3 0;
v0x6457ccbbddd0_0 .net "ori", 0 0, v0x6457ccbbe1a0_0;  1 drivers
v0x6457ccbbdea0_0 .net "rst", 0 0, v0x6457ccbbe2a0_0;  1 drivers
E_0x6457ccbac5e0 .event posedge, v0x6457ccb70490_0;
    .scope S_0x6457ccbab8d0;
T_0 ;
    %wait E_0x6457ccbac5e0;
    %load/vec4 v0x6457ccbbdea0_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 0, 0, 4;
    %assign/vec4 v0x6457ccbbdcf0_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x6457ccbbddd0_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.2, 8;
    %pushi/vec4 10, 0, 4;
    %assign/vec4 v0x6457ccbbdcf0_0, 0;
    %jmp T_0.3;
T_0.2 ;
    %load/vec4 v0x6457ccbbdcf0_0;
    %parti/s 1, 0, 2;
    %load/vec4 v0x6457ccbbdcf0_0;
    %parti/s 3, 1, 2;
    %concat/vec4; draw_concat_vec4
    %assign/vec4 v0x6457ccbbdcf0_0, 0;
T_0.3 ;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x6457ccbab740;
T_1 ;
    %delay 5, 0;
    %load/vec4 v0x6457ccbbe010_0;
    %inv;
    %store/vec4 v0x6457ccbbe010_0, 0, 1;
    %jmp T_1;
    .thread T_1;
    .scope S_0x6457ccbab740;
T_2 ;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x6457ccbbe010_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x6457ccbbe2a0_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x6457ccbbe1a0_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x6457ccbbe2a0_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x6457ccbbe1a0_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x6457ccbbe1a0_0, 0, 1;
    %delay 100, 0;
    %vpi_call 2 11 "$finish" {0 0 0};
    %end;
    .thread T_2;
    .scope S_0x6457ccbab740;
T_3 ;
    %vpi_call 2 14 "$dumpfile", "out_ringc" {0 0 0};
    %vpi_call 2 15 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x6457ccbab740 {0 0 0};
    %vpi_call 2 16 "$monitor", "time=%0t,clk=%b,rst=%b,ori=%b,count=%b", $time, v0x6457ccbbe010_0, v0x6457ccbbe2a0_0, v0x6457ccbbe1a0_0, v0x6457ccbbe0d0_0 {0 0 0};
    %end;
    .thread T_3;
# The file index is used to find the file name in the following table.
:file_names 4;
