 .timescale 0 0;
P_0x622f6d26b620 .param/l "n" 0 2 2, +C4<00000000000000000000000000001010>;
v0x622f6d2b87f0_0 .var "clk", 0 0;
v0x622f6d2b88c0_0 .net "count", 3 0, v0x622f6d2b85c0_0;  1 drivers
v0x622f6d2b8990_0 .var "rst", 0 0;
S_0x622f6d26b6c0 .scope module, "uut" "modncount" 2 5, 3 1 0, S_0x622f6d26b490;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /INPUT 1 "rst";
    .port_info 2 /OUTPUT 4 "count";
P_0x622f6d26b8a0 .param/l "n" 0 3 2, +C4<00000000000000000000000000001010>;
v0x622f6d2a6030_0 .net "clk", 0 0, v0x622f6d2b87f0_0;  1 drivers
v0x622f6d2b85c0_0 .var "count", 3 0;
v0x622f6d2b86a0_0 .net "rst", 0 0, v0x622f6d2b8990_0;  1 drivers
E_0x622f6d2a66a0 .event posedge, v0x622f6d2a6030_0;
    .scope S_0x622f6d26b6c0;
T_0 ;
    %wait E_0x622f6d2a66a0;
    %load/vec4 v0x622f6d2b86a0_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 0, 0, 4;
    %assign/vec4 v0x622f6d2b85c0_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x622f6d2b85c0_0;
    %pad/u 32;
    %cmpi/e 9, 0, 32;
    %jmp/0xz  T_0.2, 4;
    %pushi/vec4 0, 0, 4;
    %assign/vec4 v0x622f6d2b85c0_0, 0;
    %jmp T_0.3;
T_0.2 ;
    %load/vec4 v0x622f6d2b85c0_0;
    %subi 1, 0, 4;
    %assign/vec4 v0x622f6d2b85c0_0, 0;
T_0.3 ;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x622f6d26b490;
T_1 ;
    %delay 5, 0;
    %load/vec4 v0x622f6d2b87f0_0;
    %inv;
    %store/vec4 v0x622f6d2b87f0_0, 0, 1;
    %jmp T_1;
    .thread T_1;
    .scope S_0x622f6d26b490;
T_2 ;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x622f6d2b87f0_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x622f6d2b8990_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x622f6d2b8990_0, 0, 1;
    %delay 10, 0;
    %delay 100, 0;
    %vpi_call 2 11 "$finish" {0 0 0};
    %end;
    .thread T_2;
    .scope S_0x622f6d26b490;
T_3 ;
    %vpi_call 2 14 "$dumpfile", "out_modncount" {0 0 0};
    %vpi_call 2 15 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x622f6d26b490 {0 0 0};
    %vpi_call 2 16 "$monitor", "time=%0t,clk=%b,rst=%b,count=%b", $time, v0x622f6d2b87f0_0, v0x622f6d2b8990_0, v0x622f6d2b88c0_0 {0 0 0};
    %end;
